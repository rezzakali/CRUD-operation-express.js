{
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "quotes": "off",
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0
  },
  "plugins": ["prettier"]
}
